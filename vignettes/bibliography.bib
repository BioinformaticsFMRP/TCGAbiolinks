@article{Pri10,
    author = {Prill, R.J. AND Marbach, D. AND Saez-Rodriguez, J. AND Sorger, P.K. AND Alexopoulos, L.G. AND Xue, X. AND Clarke, N.D. AND Altan-Bonnet, G. AND Stolovitzky, G.},
    journal = {PLoS ONE},
