  keyword      = {INTEGRATION,CHALLENGES,MODELS,REGRESSION,RECONSTRUCTION,SELECTION,EXPRESSION DATA,TRANSCRIPTIONAL REGULATORY NETWORK,ESCHERICHIA-COLI,ALGORITHM},
  language     = {eng},
  number       = {8},
